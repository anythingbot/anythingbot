function getKey(keyCode,shiftKey) {
    if (shiftKey) {
        if (keyCode >= 65 && keyCode <= 90) {
            return chr(keyCode);
        } else if (keyCode >= 48 && keyCode <= 57) {
            return decodeShiftNumberArray[keyCode - 48];
        }
        // handle punctuation
        return '?';
    }
    // ! shiftKey
    if (keyCode >= 65 && keyCode <= 90) {
        return chr(keyCode + 32);
    } else if (keyCode >= 48 && keyCode <= 57) {
        return chr(keyCode);
    }
    // handle punctuation
    return '?';
}

